{
  "name": "@clevercloud/components",
  "version": "10.4.0",
  "description": "Collection of Web Components by Clever Cloud",
  "license": "Apache-2.0",
  "keywords": [
    "components",
    "clever-cloud"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/CleverCloud/clever-components.git"
  },
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "components:build": "rollup -c rollup/rollup-npm.config.js",
    "components:build-cdn": "rollup -c rollup/rollup-cdn.config.js",
    "components:build-cdn:versions-list": "node tasks/update-versions-list.js",
    "components:publish-cdn": "node tasks/publish-cdn.js",
    "components:check-i18n": "node tasks/check-i18n.js",
    "components:check-type-imports": "node tasks/check-type-imports.js",
    "components:check-lit": "lit-analyzer src",
    "components:docs": "cem analyze --outdir dist",
    "test": "wtr",
    "test:watch": "wtr --watch",
    "test:cem": "mocha cem/test/*.js",
    "test:cem:watch": "mocha --watch cem/test/*.js",
    "lint": "eslint --rulesdir eslint-rules rollup/*.js cem src tasks test wds",
    "lint:fix": "eslint --rulesdir eslint-rules --fix rollup/*.js cem src tasks test wds",
    "prepack": "npm run test && npm run lint && npm run components:check-lit && npm run typecheck && npm run components:check-i18n && npm run components:build && npm run components:check-type-imports && npm run components:docs && npm run stylelint",
    "prestorybook:build": "npm run components:docs",
    "preview:list": "node tasks/preview.js list",
    "preview:publish": "node tasks/preview.js publish",
    "preview:delete": "node tasks/preview.js delete",
    "start": "static -H '{\"cache-control\": \"no-cache, no-store, must-revalidate\", \"Pragma\": \"no-cache\", \"Expires\": \"0\"}' -a 0.0.0.0 -p 8080 storybook-static",
    "storybook:build": "build-storybook -o storybook-static${STORYBOOK_PATH}",
    "storybook:dev": "wds",
    "stylelint": "stylelint --config ./stylelint.config.json src/components/*/*.js",
    "stylelint:ci": "stylelint --config ./stylelint.config.json src/components/*/*.js --formatter github",
    "stylelint:fix": "stylelint --config ./stylelint.config.json src/components/*/*.js --fix",
    "typecheck": "tsc -p tsconfig-typecheck-jsdoc.json"
  },
  "dependencies": {
    "@clevercloud/client": "^7.11.0",
    "@lit-labs/motion": "^1.0.3",
    "@shoelace-style/shoelace": "^2.0.0-beta.47",
    "chart.js": "^3.5.0",
    "chartjs-plugin-datalabels": "^2.0.0",
    "immer": "^9.0.15",
    "leaflet": "^1.7.1",
    "lit": "^2.3.1",
    "rxjs": "^7.2.0",
    "statuses": "^2.0.1"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.29.0",
    "@babel/core": "^7.14.0",
    "@babel/preset-react": "^7.13.13",
    "@bundled-es-modules/chai": "^4.2.2",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@custom-elements-manifest/analyzer": "^0.6.4",
    "@open-wc/dev-server-hmr": "^0.1.3",
    "@open-wc/testing": "^3.1.6",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^18.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@web/dev-server": "^0.1.34",
    "@web/dev-server-rollup": "^0.3.19",
    "@web/dev-server-storybook": "^0.5.4",
    "@web/rollup-plugin-import-meta-assets": "^1.0.7",
    "@web/test-runner": "^0.14.0",
    "@web/test-runner-commands": "^0.6.4",
    "@web/test-runner-mocha": "^0.7.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-svg": "^1.2.0",
    "babel-plugin-template-html-minifier": "^4.1.0",
    "chai": "^4.3.6",
    "chalk": "^2.4.2",
    "clean-css": "^4.2.1",
    "dependency-graph": "^0.11.0",
    "esbuild": "^0.12.16",
    "eslint": "^7.25.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-lit": "^1.2.4",
    "eslint-plugin-lit-a11y": "^2.2.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "github-markdown-css": "^4.0.0",
    "glob": "^7.1.7",
    "hanbi": "^1.0.1",
    "highlight.js": "^10.7.2",
    "i18n-extract": "^0.6.7",
    "js-yaml": "^4.1.0",
    "lit-analyzer": "^1.2.1",
    "mime-types": "^2.1.30",
    "mocha": "^10.0.0",
    "node-static": "^0.7.11",
    "postcss-styled": "^0.34.0",
    "remark-frontmatter": "^3.0.0",
    "remark-gfm": "^1.0.0",
    "remark-highlight.js": "^6.0.0",
    "remark-html": "^13.0.1",
    "remark-parse": "^9.0.0",
    "remixicon": "^2.5.0",
    "rollup": "^2.47.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.5.0",
    "stylelint": "^14.15.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-standard": "^26.0.0",
    "superagent": "^6.1.0",
    "svgo": "^1.3.2",
    "text-table": "^0.2.0",
    "typescript": "~4.3.2",
    "unified": "^9.2.1"
  },
  "customElements": "dist/custom-elements.json",
  "volta": {
    "node": "18.13.0",
    "npm": "8.19.3"
  }
}
